{
  "name": "@ja-to-en/ui-content",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/content.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --out-dir dist --sourcemap && mv dist/index.js dist/content.js",
    "dev": "tsup src/index.ts --format esm --out-dir dist --sourcemap --watch",
    "test": "vitest run --passWithNoTests",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'"
  },
  "dependencies": {
    "@ja-to-en/domain": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
